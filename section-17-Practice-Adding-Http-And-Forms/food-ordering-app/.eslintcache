nst responseData = await response.json();\n      //I got an object but I want an array\n      const loadedMeals = [];\n\n      for (const mealKey in responseData) {\n        loadedMeals.push({\n          id: mealKey,\n          name: responseData[mealKey].name,\n          description: responseData[mealKey].description,\n          price: responseData[mealKey].price,\n        });\n      }\n      dispatch({ type: ACTIONS.addMeals, meals: loadedMeals });\n\n      // setMeals(loadedMeals);\n      // setIsLoading(false);\n    };\n    fetchMeals().catch((error) =>\n      dispatch({ type: ACTIONS.toggleError, error: error })\n    );\n  }, []); //No dependencies so it runs only on load\n\n  if (mealState.error) {\n    return (\n      <section className={classes.MealsError}>\n        <p>{mealState.error}</p>\n      </section>\n    );\n  }\n  if (mealState.isLoading) {\n    return (\n      <section className={classes.MealsLoading}>\n        <p>Loading available meals</p>\n      </section>\n    );\n  }\n\n  const mealsList = mealState.meals.map((meal) => (\n    <MealItem\n      key={meal.id}\n      id={meal.id}\n      name={meal.name}\n      description={meal.description}\n      price={meal.price}\n    />\n  ));\n\n  //USING A CUSTOM HTTP HOOK:\n  /*\n  const [mealsList, setMealsList] = useState([]);\n  const { isLoading, error, sendRequest: fetchData } = useHttp();\n\n  useEffect(() => {\n    const requestConfig = {\n      url: \"https://react-js-course-b4b3c-default-rtdb.firebaseio.com/meals.json\",\n    };\n    const transformMeals = (mealsObj) => {\n      const loadedMeals = [];\n\n      for (const mealKey in mealsObj) {\n        loadedMeals.push({\n          id: mealKey,\n          name: mealsObj[mealKey].name,\n          description: mealsObj[mealKey].description,\n          price: mealsObj[mealKey].price,\n        });\n      }\n\n      const mealsListJSX = loadedMeals.map((meal) => (\n        <MealItem\n          key={meal.id}\n          id={meal.id}\n          name={meal.name}\n          description={meal.description}\n          price={meal.price}\n        />\n      ));\n\n      setMealsList(mealsListJSX);\n    };\n\n    fetchData(requestConfig, transformMeals);\n  }, [fetchData]);\n  */\n\n  return (\n    <section className={classes.meals}>\n      <Card>{mealsList}</Card>\n    </section>\n  );\n};\n\nexport default AvailableMeals;\n","/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Layout/HeaderCartButton.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/UI/Modal.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/UI/Card.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Cart/CartIcon.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Meals/MealItem/MealItem.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Meals/MealItem/MealItemForm.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/UI/Input.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/hooks/use-http.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Cart/Input.js",[],"/home/jan/Documents/reactjs-course/section-17-Practice-Adding-Http-And-Forms/food-ordering-app/src/components/Cart/CheckOut.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":4,"column":8,"nodeType":"94","messageId":"95","endLine":4,"endColumn":15},{"ruleId":"92","severity":1,"message":"96","line":5,"column":10,"nodeType":"94","messageId":"95","endLine":5,"